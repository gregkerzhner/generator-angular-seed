{
  "name": "<%= appName %>",
  "version": "0.0.1",
  "dependencies": {
    "angular": "<%= angularVersion %>",
    <% if(jQueryVersion){%>
    "jquery": "<%=jQueryVersion%>",
    <% } %>

    <% if(bootstrapVersion){%>
      "bootstrap": "<%=bootstrapVersion%>",
    <% } %>
    "angular-sanitize": "<%=angularVersion%>",
    "angular-animate": "<%=angularVersion%>",
    <% if(uiRouterVersion){%>
      "angular-ui-router": "<%=uiRouterVersion%>",
    <% } %>
    <% if(underscoreVersion){%>
    "underscore": "<%= underscoreVersion %>",
    <% }%>
    "angular-cookies": "<%=angularVersion%>"
  }
}
